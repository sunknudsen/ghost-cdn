{
  "name": "ghost-cdn",
  "version": "1.0.1",
  "description": "Deliver content on Ghost.",
  "main": "index.js",
  "scripts": {
    "code": "nodemon",
    "ncu": "ncu --target minor --upgrade"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunknudsen/ghost-cdn.git"
  },
  "author": "Sun Knudsen <hello@sunknudsen.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunknudsen/ghost-cdn/issues"
  },
  "homepage": "https://github.com/sunknudsen/ghost-cdn#readme",
  "dependencies": {
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.3",
    "fs-extra": "^10.0.1",
    "got": "^11.8.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "npm-check-updates": "^12.5.4"
  },
  "nodemonConfig": {
    "delay": 1000,
    "ext": "js",
    "watch": [
      "index.js"
    ],
    "exec": "node index.js"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
